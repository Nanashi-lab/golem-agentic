{
  "name": "@golemcloud/golem-ts-sdk",
  "type": "module",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public",
    "tag": "test"
  },
  "files": [
    "types",
    "dist"
  ],
  "main": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "typesVersions": {
    "*": {
      "agentic-guest": [
        "types/exports.d.ts"
      ],
      "golem:agent/common": [
        "types/golem_agent_common.d.ts"
      ],
      "golem:agent/host": [
        "types/golem_agent_host.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*\" \"tests/**/*\"",
    "format:check": "prettier --check \"src/**/*\" \"tests/**/*\"",
    "generate-dts": "wasm-rquickjs generate-dts --wit ../../../wit --output types",
    "clean-test-data": "rm -rf .metadata",
    "generate-test-types-metadata": "pnpm exec golem-typegen ./tsconfig.json --files tests/**/*.ts",
    "prebuild": "npm run clean-test-data && pnpm run generate-dts && pnpm run generate-test-types-metadata",
    "build": "npm run prebuild && tsc --noEmit && rollup -c",
    "clean": "rm -rf .metadata dist node_modules package-lock.json && printf '\\nRun `npm install` before building again.\\n\\n'",
    "test": "pnpm run clean-test-data && npm run generate-test-types-metadata && tsc --noEmit && vitest run",
    "dev": "rollup -c -w"
  },
  "keywords": [],
  "sideEffects": [
    "src/index.ts"
  ],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@golemcloud/golem-ts-typegen": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "fast-check": "^4.2.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.1.14",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-morph": "^26.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "private": false,
  "dependencies": {
    "@golemcloud/golem-ts-types-core": "workspace:*",
    "effect": "^3.17.6"
  }
}