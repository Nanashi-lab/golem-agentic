{
  "name": "@golemcloud/golem-ts-reflect",
  "type": "module",
  "version": "0.0.1-dev.1",
  "publishConfig": {
    "access": "public",
    "tag": "test"
  },
  "files": [
    "types",
    "../../dist"
  ],
  "main": "../../dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "scripts": {
    "lint": "../../node_modules/.bin/eslint . --ext .ts,.tsx",
    "lint:fix": "../../node_modules/.bin/eslint . --ext .ts,.tsx --fix",
    "format": "../../node_modules/.bin/prettier --write \"src/**/*\" \"tests/**/*\"",
    "format:check": "../../node_modules/.bin/prettier --check \"src/**/*\" \"tests/**/*\"",
    "test": "../../node_modules/.bin/tsc --noEmit && vitest run",
    "build": "../../node_modules/.bin/tsc --noEmit && rollup -c",
    "dev": "../../node_modules/.bin/rollup -c -w"
  },
  "keywords": [],
  "sideEffects": [
    "src/index.ts"
  ],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "fast-check": "^4.2.0",
    "prettier": "^3.6.2",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-morph": "^26.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "private": false
