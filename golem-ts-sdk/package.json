{
  "name": "@golemcloud/golem-ts-sdk",
  "type": "module",
  "version": "0.0.1-dev.17",
  "publishConfig": {
    "access": "public",
    "tag": "test"
  },
  "files": [
    "types",
    "dist"
  ],
  "main": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "typesVersions": {
    "*": {
      "agentic-guest": [
        "types/exports.d.ts"
      ],
      "golem:agent/common": [
        "types/golem_agent_common.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*\" \"tests/**/*\"",
    "format:check": "prettier --check \"src/**/*\" \"tests/**/*\"",
    "test": "npx @rttist/typegen@0.2.0 generate --force && tsc --noEmit && vitest run",
    "prebuild": "wasm-rquickjs generate-dts --wit ../wit --output types",
    "build": "npm run prebuild && npx @rttist/typegen@0.2.0 generate --force && tsc --noEmit && rollup -c",
    "dev": "rollup -c -w"
  },
  "keywords": [],
  "sideEffects": [
    "src/index.ts"
  ],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "fast-check": "^4.2.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.1.14",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "private": false,
  "dependencies": {
    "effect": "^3.17.6",
    "rttist": "^1.0.0-rc.4"
  }
}
